                "Update password": {
                  "value": {
                    "password": "NewP@ssw0rd123"
                  }
                },
                "Update admin status": {
                  "value": {
                    "isAdmin": true
                  }
                },
                "Update profile": {
                  "value": {
                    "userName": "john_doe",
