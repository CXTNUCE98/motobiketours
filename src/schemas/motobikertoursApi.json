                "Full update": {
                  "value": {
                    "userName": "john_doe",
                    "email": "john.doe@example.com",
                    "password": "NewP@ssw0rd123",
                    "isAdmin": true,
                    "timezone": "America/New_York",
                    "avatar": "https://res.cloudinary.com/demo/image/upload/avatar.jpg"
                  }
